{
  "manifest_version": 3,
  "name": "Knowledge.Ai",
  "description": "Turn documents to mindmaps!",
  "version": "1.0",
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  },
  "action": {
    "default_icon": "icon.png",
    "default_title": "Knowledge.Ai",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["contextMenus"]
}
